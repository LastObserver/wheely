{
  "plugins": ["angular"],
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "rules": {
    "no-param-reassign": ["error", {
      "props": false
    }],
    "semi": ["error","always"],
    "no-await-in-loop": 0,
    "class-methods-use-this": 0,
  },
  "env": {
    "browser": true,
    "node": true
  }
}
